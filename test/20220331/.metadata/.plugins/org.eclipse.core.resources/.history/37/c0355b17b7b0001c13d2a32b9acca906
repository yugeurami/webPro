package com.test2;

public class Student extends Person {
	public static int num = 0;
	private String ban;
	public Student(String id, String name, String ban) {
		super(id, name);
		this.ban = ban;
		num++;
		super.setId("A0" + num);
	}
	@Override
	public String toString() {
		return super.toString() + "\t(ë°˜)" + ban;
	}
	
}
